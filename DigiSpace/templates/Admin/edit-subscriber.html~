function validateBusinessCategory(mydiv){

var radioValue = $("input[name='category']:checked").val();
if(radioValue)
{
   $(mydiv).parent().children('.error').css("display", "none");
   return true;
}
else{
	$(mydiv).parent().children('.error').css("display", "block");
   $(mydiv).parent().children('.error').text("Please select Business");
   return false; 
}
}

function validateService(service){

var radioValue = $("input[name='service']:checked").val();
if(radioValue)
{
   $(service).parent().children('.error').css("display", "none");
   return true;
}
else{
	$(service).parent().children('.error').css("display", "block");
   $(service).parent().children('.error').text("Please select Service");
   return false; 
}
}

function validateDuration(duration){

var radioValue = $("input[name='duration']:checked").val();
if(radioValue)
{
   $(duration).parent().children('.error').css("display", "none");
   return true;
}
else{
	$(duration).parent().children('.error').css("display", "block");
   $(duration).parent().children('.error').text("Please select Duration");
   return false; 
}
}

function validateDate(){


if($("#dt1").val()!='' || $("#dt3").val()!='' || $("#dt5").val()!='' || $("#dt7").val()!='' || $("#dt9").val()!='')
{
   $(duration).parent().children('.error').css("display", "none");
   return true;
}
else{
	$(duration).parent().children('.error').css("display", "block");
   $(duration).parent().children('.error').text("Please select Start Date");
   return false; 
}
}

function validateDate1(){
var result = isStartDateTextfieldEmpty();
if(result==false){
	$('#startdateemptyerror').text("Please select Start Date");
   $('#startdateemptyerror').css("display", "block");
   return false;
}
else{
  $('#startdateemptyerror').css("display", "none");
  	return true;
}
}
	
 function isStartDateTextfieldEmpty(){

var status= true;
 $.each($("input[name='premium_service']:checked"), function(){
          var value = $(this).parents().eq(4).next('div').find('input').val().trim();
         	
          if(value != '')
          {
           return true;        
          }
          else{
 		
           status = false;
           return false;
          }
	           
   });

return status;	
}	
	
function isDayEmpty(){

var status= true;
 $.each($("input[name='premium_service']:checked"), function(){
		id = $(this).parents().eq(4).next('div').find('input').attr('id');
		var value = $('#'+id).parent('div').next('div').find('select').val(); 
          if(value != null)
          {
           return true;        
          }
          else{	
           status = false;
           return false;
          }
	           
   });
return status;	
}		
	
	

function checkDays(){
x=0;
 $.each($("input[name='premium_service']:checked"), function(){
id = $(this).parents().eq(4).next('div').find('input').attr('id');
id1 = $('#'+id).parent('div').next('div').find('select').val();
val = isNaN(parseInt(id1))? 0 : parseInt(id1)
x = x + val
 });
if(x<=totalDays)
{
   $('#mismatch-days').css("display", "none");
   return true;
}
else{
	$('#mismatch-days').css("display", "block");
   $('#mismatch-days').text("Premium service days should be less than or equal to Duration");
   return false; 
}
}	

function checkDate()
{
if(validateDate("#duration")&validateDate1())
{
return true;
}
return false;
}
	
		

function days(){

var result = isDayEmpty();
if(result==false){
	$(premium_service).parent().children('.error').css("display", "block");
   $(premium_service).parent().children('.error').text("Please select No of Days");
   return false;
}
else{
   $(premium_service).parent().children('.error').css("display", "none");
  	return true;
}
}

function checkService(){

	if(validateBusinessCategory("#mydiv")&validateService("#service")&validateDuration("#duration")&validateDate("#duration")&validateDate1()){
		return true;	
	}
	return false;
}

$("#save-service-detail").click(function(event){

 event.preventDefault(); 
	if(checkService()){
	if(checkDate()){	
		if(days()){
		if(checkDays()){
		var premium_day_list = [];
		var premium_service_list = [];
		var premium_start_date=[];
		var premium_end_date=[];
      $.each($("input[name='premium_service']:checked"), function(){            
                premium_day_list.push($(this).parents().eq(4).next('div').next('div').find('select').val());           			                 
                premium_service_list.push($(this).val());
                premium_start_date.push($(this).parents().eq(4).next('div').find('input').val());           			                    
                premium_end_date.push($(this).parents().eq(4).next('div').next('div').next('div').find('input').val());           			                    

       });


	   var formData= new FormData();

		formData.append("selected_duration",selected_duration);
		formData.append("duration_start_date",duration_start_date);
		formData.append("duration_end_date",duration_end_date);
		formData.append("premium_day_list",premium_day_list);
		formData.append("premium_start_date",premium_start_date);
		formData.append("premium_end_date",premium_end_date);


		formData.append("service",$("input[name='service']:checked").val());
		formData.append("category",$("input[name='category']:checked").val());
		formData.append("duration",$("input[name='duration']:checked").val());
		formData.append("premium_service",premium_service_list);
		formData.append("user_email",$('#user_email').val());

		
		$.ajax({
			  type	: "POST",
			   url : '/edit-service/',
				 data : formData,
					cache: false,
		         processData: false,
		    		contentType: false,                       
                       
		     success: function (response) {
		        if(response.success=='true'){
		        $("#affiliate-modal1").modal('show');
		        $("#transaction_code1").text(response.transaction_code);
		        	}
					if (response.success == "false") {
						$("#error-modal1").modal('show');
						$("#error-message1").text(response.message);                    
		 			}							                        		                      		                       		
		      },
		      	beforeSend: function () {
            $("#processing").css('display','block');
            },
            complete: function () {
                $("#processing").css('display','none');
            },
		      error : function(response){
		         	alert("_Error");
		   	}              
		  });   	
	}
	}
	}
	}
});


/*checking start date and end date*/
function validateStartAndEndDates(){
    var status = true;
    var msg = "";
     $.each($("input[name='premium_service']:checked"), function(){            
               var startdate = $(this).parents().eq(4).next('div').find('input').val();
               var enddate = $(this).parents().eq(5).children().last().find('input').val();
           
                if(startdate == ''){
                    status = false;
                    msg = "Please select advert start date";
                    return false;
                }

                else if(enddate == ''){
                    status = false;
                    msg = "Please select advert end date";
                    return false;
                }
               
       });
     if(!status){
     $("#mismatch-days").text(msg);
     $("#mismatch-days").css('display','block');
        }
        else{
    $("#mismatch-days").css('display','none');
        }
     return status;
}

subscriber_id = '';


$("#continue-service-detail").click(function(event)  {

		event.preventDefault();
		
	if(checkService()){	
		if(checkDate()){	
			if(days()){
			if(checkDays()){
		var premium_day_list = [];
		var premium_service_list = [];
		var premium_start_date = [];
		var premium_end_date = [];

      $.each($("input[name=premium_service]:checked"), function(){  
                premium_day_list.push($(this).parents().eq(4).next('div').next('div').find('select').val());           			          
                premium_service_list.push($(this).val());
                premium_start_date.push($(this).parents().eq(4).next('div').find('input').val());           			                    
                premium_end_date.push($(this).parents().eq(4).next('div').next('div').next('div').find('input').val());           			                    
       });

	   var formData= new FormData();

		formData.append("selected_duration",selected_duration);
		formData.append("duration_start_date",duration_start_date);
		formData.append("duration_end_date",duration_end_date);
		formData.append("premium_day_list",premium_day_list);
		formData.append("premium_start_date",premium_start_date);
		formData.append("premium_end_date",premium_end_date);


		formData.append("service",$("input[name='service']:checked").val());
		formData.append("category",$("input[name='category']:checked").val());
		formData.append("duration",$("input[name='duration']:checked").val());
		formData.append("premium_service",premium_service_list);
		formData.append("user_email",$('#user_email').val());
	
		$.ajax({
			  type	: "POST",
			   url : '/edit-service/',
				 data : formData,
					cache: false,
		         processData: false,
		    		contentType: false,                       
                       
		     success: function (response) {
		        if(response.success=='true'){
		        $("#affiliate-modal2").modal('show');
		        $("#transaction_code2").text(response.transaction_code);
		        subscriber_id = response.subscriber_id
		        	}
					if (response.success == "false") {
						$("#error-modal1").modal('show');
						$("#error-message1").text(response.message);                                        
		 			}							                        		                      		                       		
		      },
		      	beforeSend: function () {
            $("#processing").css('display','block');
            },
            complete: function () {
                $("#processing").css('display','none');
            },
		      error : function(response){
		         	alert("_Error");
		   	}              
		  });			
}
}
}
 	}

});

function redirect_to_retails()
{
	$("#affiliate-modal2").modal('hide');
		var premium_service_list = [];
		var premium_day_list = [];
      $.each($("input[name=premium_service]:checked"), function(){  
                premium_day_list.push(($(this).parents().eq(4).next('div').next('div').find('select').val())+ ' Days');           			          
                premium_service_list.push($(this).val());
       });

   
		 $("#form_wizard_1").bootstrapWizard('show',2);


		  $('#show_subscriber_id').text(subscriber_id);		 
		  $('#show_business_name').text($('#business_name').val());		 
		  $('#show_category').text($("input[name='category']:checked").val());
		  $('#show_service').text($("input[name='service']:checked").val());
		  $('#show_duration').text($("input[name='duration']:checked").val() + ' Days' );
		 $('#show_premium_service_duration').text(premium_day_list);
		$('#show_premium_service').text(premium_service_list);		 


}


