$('#'+id).removeAttr("disabled");
$('#'+id).select2('val','0');
$('#'+id).parent('div').next('div').find('input').val('');
});


$('.duration').change(function(e){
e.preventDefault();
prevDate = $(this).val()

var id = $(this).parent('div').parent('ul:first-child').find('input').val();


//var nextDate = $(this).parent('div').next('div').children('input').datepicker('getDate');
var days = parseInt(id) -1

selected_duration = id;
duration_start_date = prevDate
date =  new Date(prevDate);
date =  new Date(prevDate);
date.addDays(days);
//date =  (date.getMonth() + 1) + '/' + date.getDate() + '/' +  date.getFullYear();
var mo =  ((date.getMonth() + 1) < 10) ?'0'+(date.getMonth() + 1) : (date.getMonth() + 1);
var d = (date.getDate() < 10) ? '0'+date.getDate() : date.getDate();

date =   mo+'/' + d + '/' +  date.getFullYear();


duration_end_date = date; 
setDatePicker(duration_start_date,duration_end_date)
$(this).parent('div').next('div').next('div').find('input').val(date);

 $('.premium_checkbox').each(function( index ) {
 	$(this).removeAttr('disabled');
	$( this ).parents().eq(1).removeClass('disabled');
  
});
});

$('#country option[value='+ $('#selectedCountry').val() +']').attr("selected", "selected");
$('#state option[value='+ $('#selectedState').val() +']').attr("selected", "selected");
$('#city option[value='+ $('#selectedCity').val() +']').attr("selected", "selected");


function getState() {
	$('#state').select2('val','');
     country_id = $('#country :selected').val();
    if (country_id) {
        $.ajax({
            type: 'GET',
            url: '/get-state/',
            data: {'country_id': country_id},
            success: function (response) {
                    //alert('success');
                $('#state').html('');
                $.each(response.state_list, function (index, item) {
                    $('#state').append(item);
                });
                $('#state').val("");
            },
            error: function (response) {
                alert("Error!");
            },

        });
    }
  
}

function getPincode(){
			$('#pincode').select2('val','');
        city_id = $('#city :selected').val();
        if (city_id) {
            $.ajax({
                type: 'GET',
                url: '/get-pincode/',
                data: {'city_id': city_id},
                 success: function (response) {
                    $('#pincode').html('');
                    $.each(response.pincode_list, function (index, item) {
                        $('#pincode').append(item);
                    });
                    $('#pincode').val("");
                },
                error: function (response) {
                    alert("Error!");
                },

            });
        }
        else {
            $('#city').val("");
        }
    }


function getCity() {
			$('#city').select2('val','');
			$('#pincode').select2('val','');
        state_id = $('#state :selected').val();
        if (state_id) {
            $.ajax({
                type: 'GET',
                url: '/get-city/',
                data: {'state_id': state_id},
                success: function (response) {
                		//alert('success');
                    $('#city').html('');
                    $.each(response.city_list, function (index, item) {
                        $('#city').append(item);
                    });
                    $('#city').val("");
                },
                error: function (response) {
                    alert("Error!");
                },

            });
        }
        else {
            $('#city').val("");
        }
    }

function checkBusinessName(business_name){
	businessName = $(business_name).val()   
  
   if(businessName!=''){
 	$(business_name).parent().children('.error').css("display", "none");
   return true;
   }else{
    $(business_name).parent().children('.error').css("display", "block");
    $(business_name).parent().children('.error').text("Please enter Business Name");
   return false; 
   }
}


function checkNumber(phone_no){
	phoneno = $(phone_no).val()     
   var phoneNumberPattern = /^[789]\d{9}$/;  
   if(phoneNumberPattern.test(phoneno)){
 	$(phone_no).parent().children('.error').css("display", "none");
   return true;
   }else{
    $(phone_no).parent().children('.error').css("display", "block");
    $(phone_no).parent().children('.error').text("Please enter valid Phone No");
   return false; 
   }
}

function checkSecNumber(sec_phone_no){
	phoneno = $(sec_phone_no).val()     
   var phoneNumberPattern = /^[789]\d{9}$/; 
  if(phoneno=='')
	{
	return true;	
	} 
   else if(phoneNumberPattern.test(phoneno)){
 	$(sec_phone_no).parent().children('.error').css("display", "none");
   return true;
   }else{
    $(sec_phone_no).parent().children('.error').css("display", "block");
    $(sec_phone_no).parent().children('.error').text("Please enter valid Phone No");
   return false; 
   }
}

function checkEmail(pri_email){
	Email = $(pri_email).val()
   var namePattern = /[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$/;  
 
   if(namePattern.test(Email)){
      $(pri_email).parent().children('.error').css("display", "none");
   return true;
   }else{
 	$(pri_email).parent().children('.error').css("display", "block");
   $(pri_email).parent().children('.error').text("Please enter valid Email");
   return false; 
   }
}
	
function checkSecEmail(sec_email){
	Email = $(sec_email).val()
   var namePattern = /[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$/;  
 	 if(Email=='')
	{
	return true;	
	} 
   else if(namePattern.test(Email)){
      $(sec_email).parent().children('.error').css("display", "none");
   return true;
   }else{
 	$(sec_email).parent().children('.error').css("display", "block");
   $(sec_email).parent().children('.error').text("Please enter valid Email");
   return false; 
   }
}
	
function checkLogo(logo){
resume = $(logo).val()
   
if(resume=='')
{
 	  $(logo).parent().children('.error').css("display", "none");
	     return true;
}	
else 
{
var val = $(logo).val().toLowerCase();
var regex = new RegExp("([a-zA-Z0-9\s_\\.\-:])+(.png|.jpeg|.jpg)$");
if(!(regex.test(val))) {
 	$(logo).parent().children('.error').css("display", "block");
   $(logo).parent().children('.error').text("Please upload an Image");
     return false; 
}
else {
      $(logo).parent().children('.error').css("display", "none");
	     return true;
	}
} 
}	


function checkaddress(address1){

	if($(address1).val()!='')
   {
    $(address1).parent().children('.error').css("display", "none");
   return true;
   }else{
    $(address1).parent().children('.error').css("display", "block");
    $(address1).parent().children('.error').text("Please enter valid Address Line 1");
   return false; 
   }
}
		function checkcountry(country){

  if($(country).val()!='' && $(country).val()!= null)
   {

   	
    $(country).parent().children('.error').css("display", "none");
   return true;
   }else{

    $(country).parent().children('.error').css("display", "block");
    $(country).parent().children('.error').text("Please select Country");
   return false; 
   }
}

function checkState(state){



  if($(state).val()!='' && $(state).val()!=null)
   {
   
    $(state).parent().children('.error').css("display", "none");
   return true;
   }else{
    $(state).parent().children('.error').css("display", "block");
    $(state).parent().children('.error').text("Please select State");
   return false; 
   }
}
function checkCity(city){
 if($(city).val()!='' && $(city).val()!=null)
   {
   $(city).parent().children('.error').css("display", "none");
   return true;
   }

   else {
    $(city).parent().children('.error').css("display", "block");
    $(city).parent().children('.error').text("Please select City");
   return false; 
   }
}


function checkPincode(pincode){
 if($(pincode).val()!=null && $(pincode).val()!='')
   {
    $(pincode).parent().children('.error').css("display", "none");
   return true;
   }

   else{
    $(pincode).parent().children('.error').css("display", "block");
    $(pincode).parent().children('.error').text("Please select Pincode");
   return false; 
   }
}

function checkBusiness(business){
	if($(business).val()!='')
   {
    $( business).parent().children('.error').css("display", "none");
   return true;
   }else{
    $( business).parent().children('.error').css("display", "block");
    $( business).parent().children('.error').text("Please enter about Business");
   return false; 
   }
}

	
function checkUserName(user_name){
 	var namePattern = /^[a-zA-Z\s]+$/;  
	first_name = $(user_name).val()  
   if(namePattern.test(first_name)){

 	$(user_name).parent().children('.error').css("display", "none");
   return true;
   }else{

    $(user_name).parent().children('.error').css("display", "block");
    $(user_name).parent().children('.error').text("Please enter valid Name");
   return false; 
   }
}


function checkUserContactNo(user_contact_no){

   var namePattern = /^[789]\d{9}$/;
	contactno = $(user_contact_no).val()  
   if(namePattern.test(contactno)){

 	$(user_contact_no).parent().children('.error').css("display", "none");
   return true;
   }else{

    $(user_contact_no).parent().children('.error').css("display", "block");
    $(user_contact_no).parent().children('.error').text("Please enter valid Contact No");
   return false; 
   }
}

function checkUserEmail(user_email){
	Email = $(user_email).val()
   var namePattern = /[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$/;  
 if(namePattern.test(Email)){
      $(user_email).parent().children('.error').css("display", "none");
   return true;
   }else{
 	$(user_email).parent().children('.error').css("display", "block");
   $(user_email).parent().children('.error').text("Please enter valid Email");
   return false; 
   }
}

function validateData(){

	if(checkBusinessName("#business_name")&checkNumber("#phone_no")&checkSecNumber("#sec_phone_no")&checkEmail("#pri_email")&checkSecEmail("#sec_email")&checkaddress("#address1")&checkcountry("#country")&checkState("#state")&checkCity("#city")&checkPincode("#pincode")&checkBusiness('#business')&checkUserName('#user_name')&checkUserEmail('#user_email')&checkUserContactNo('#user_contact_no')){
		return true;	
	}
	return false;
}

$("#save-quit").click(function(event)  {
	     event.preventDefault();  			
	if(validateData()){		
	  	var input = document.getElementById("logo");  	  
		file = input.files[0];
	   var formData= new FormData();

        var useremail;
       var checkusername = "{{ contact_email  }}"
       if ($('#user_email').val() == checkusername){
        useremail = ""
       
       }else{
        useremail = $('#user_email').val();
       
       }
	
		formData.append("logo",file);
        formData.append("supplier_id",$('#supplier_id').val());
		formData.append("business_name",$('#business_name').val());
		formData.append("phone_no",$('#phone_no').val());    
		formData.append("sec_phone_no",$('#sec_phone_no').val());    
		formData.append("email",$('#pri_email').val());    
		formData.append("sec_email",$('#sec_email').val());
		formData.append("address1",$('#address1').val());
		formData.append("address2",$('#address2').val());
		formData.append("state",$('#state').val());
		formData.append("city",$('#city').val());
		formData.append("pincode",$('#pincode').val());
		formData.append("business",$('#business').val());
		formData.append("user_name",$('#user_name').val());
		formData.append("user_email",$('#user_email').val());
		formData.append("user_contact_no",$('#user_contact_no').val());

  			$.ajax({
  				  
				  type	: "POST",
				   url : '/update-subscriber/',
 					data : formData,
					cache: false,
		         processData: false,
		    		contentType: false,                       
              success: function (response) {
	              if(response.success=='true'){
	           	  		$("#affiliate-modal").modal('show');
	              	}
	      			if (response.success == "false") {
							$("#error-modal").modal('show');                    
	       			}							                        		                      		                       		
               },
               	beforeSend: function () {
            $("#processing").css('display','block');
            },
            complete: function () {
                $("#processing").css('display','none');
            },
               error : function(response){
                  	alert("_Error");
            	}              
           });   
  }
});

