$('.save1').click(function(event){
event.preventDefault();

if(check_data()){


	   var formData= new FormData();

		formData.append("user_email",$('#user_email').val());

		payment_mode = $("input[name='payment_mode']:checked").val()
		formData.append("payment_mode",payment_mode);
		formData.append("payment_code",$('#payment_code').val());
		formData.append("generated_amount",$('#generated_amount').val());
		formData.append("payable_amount",$('#payable_amount').val());
        formData.append("cheque_number",$('#cheque_number').val());
        formData.append("bank_branch_name",$('#bank_branch_name').val());
        formData.append("bank_name",$('#bank_name').val());
		formData.append("paid_amount",$('#paid_amount').val());
		formData.append("note",$('#note').val());
		formData.append("selected_tax_type",selected_tax_type);



	$.ajax({
			  type	: "POST",
			   url : '/update-subscriber-detail/',
				 data : formData,
				cache: false,
		       processData: false,
		    		contentType: false,                       
                       
		     success: function (response) {
		        if(response.success=='true'){
		        $("#affiliate-modal3").modal('show');
     		     $("#transaction_code3").text(response.payment_code);
		        	}
					if (response.success == "false") {
						$("#error-modal").modal('show');                    
		 			}							                        		                      		                       		
		      },
		      	beforeSend: function () {
            $("#processing").css('display','block');
            },
            complete: function () {
                $("#processing").css('display','none');
            },
		      error : function(response){
		         	alert("_Error");
		   	}              
		  });		
		
}
});

//tax calculator
var final_q;
function changeprice_edit()

{
	if($("#tax_type").val()==15)
	{
	selected_tax_type = 'Service Tax 15%' 	
	}
	else
	{
	selected_tax_type = 'VAT 4%' 		
	}
	var price=$("#generated_amount").val();
	var tax=$("#tax_type").val();
	var amount=(tax/100)*price;
	final_q= parseFloat(price) +parseFloat(amount);
	$("#payable_amount").val(final_q);
}

 function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57 || charCode == 43 || charCode == 45 ))
            return false;
        return true;
    }

    function validateFloatKeyPresss(el, evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode;
        var number = el.value.split('+');
        var number1 = el.value.split('-');
        if (charCode != 45 && charCode != 43 && charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        //just one dot
        if (number.length > 1 && charCode == 43) {
            return false;
        }
        if (number1.length > 1 && charCode == 45) {
            return false;
        }
        return true;
    }

</script>


    </body>

