Date.prototype.addDays = function(days) {
    this.setDate(this.getDate() + parseInt(days));
    return this;
};



$('.days').on('select2:close', function(){
var prevDate = $(this).parent('div').prev('div').children('input').val();



if(prevDate!='')
{

var days = $(this).val();
if(days == null){
    days = 0;
}

var day1 = days
var day = parseInt(day1) -1;

//premium_day.push(days);
//premium_start_date.push(prevDate);

var date =  new Date(prevDate);
date.addDays(day);

value = days;


//date = (date.getMonth() + 1) + '/' + date.getDate() + '/' +  date.getFullYear();
var mo =  ((date.getMonth() + 1) < 10) ?'0'+(date.getMonth() + 1) : (date.getMonth() + 1);
var d = (date.getDate() < 10) ? '0'+date.getDate() : date.getDate();

date =   mo+'/' + d + '/' +  date.getFullYear();

//premium_end_date.push(date.toString());


if(days == 0){ 
     $(this).parent('div').next('div').children('input').removeAttr('value');
   
}else{
    $(this).parent('div').next('div').children('input').val(date);
}
}
   });  

function check_advert(id)
{
var check_id = id;

id = $('#'+check_id).parents().eq(4).next('div').find('input').attr('id');
id1 = $('#'+id).parent('div').next('div').find('select').attr('id');
id2 = $('#'+id1).parent('div').next('div').find('input').attr('id');

var flag = document.getElementById(id).hasAttribute("disabled");

if(flag == false)
{
$('#'+id).attr("disabled", 'disabled');
$('#'+id1).attr("disabled", 'disabled');
$('#'+id).val('');
$('#'+id1).select2('val','0');
$('#'+id2).val('');
}
else{
 $('#'+id).removeAttr("disabled");
}
}

$('.check-duration').change(function(e){

	e.preventDefault();
	id = $(this).closest('div').next('div').next('div').find('input').attr('id'); 
	totalDays = $(this).closest('div').find('input').val();
	
	$(".duration").attr("disabled","disabled");
	$(".service-duration").attr("disabled","disabled");		
	$(".premium_checkbox").attr("disabled","disabled");		
   $('#premium_service').find('input[type=checkbox]:checked').removeAttr('checked').closest('span').removeClass('checked');	
	$('.end-date').val('');

	if($(this).find("span").hasClass('checked')){

	 	$('#'+id).removeAttr("disabled"); 
	}
	else{

	 	$('#'+id).attr("disabled","disabled"); 
	}

	var key =  $(this).text().trim();
 		var list = referanceDays[key];
 		list = list.split(",");
 		
 		$('.premium_service1').val('');

 		$('.service-duration').val('');
 		$('.end-date').val('');
 		$('.days').html('').select2({data: {id:null, text: null}});
 		 $('.days').attr("disabled","disabled");

 		 var days = [3,7,30,90,180];
 		$.each(list, function (index, item) {
                  $('.days').append('<option value="'+days[index]+'">'+item+'</option>');
       });
}); 


